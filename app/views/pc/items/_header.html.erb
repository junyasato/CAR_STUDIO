 <%= form_for items, html: { remote: true, authenticity_token: true, multipart: true } do |f| %>
    <%= f.text_field :desc, class: "form-control", value: desc, placeholder: "見出し" %>
    <%= f.hidden_field :item_type, :value => "header" %>
    <%= f.hidden_field :car_id, :value => car_id %>
    <%= f.submit "アップロード", class: "btn btn-add-item"  %>
  <% end %>